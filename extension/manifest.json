{
    "name": "Sync Chrome Tabs",
    "version": "0.1",
    "description": "Build an Extension!",
    "manifest_version": 2,
	"background": {
		"scripts": ["tabs.js"],
		"persistent": true
	},
	"browser_action": {
        "default_title": "Show tabs"      // optional; shown in tooltip
        //"default_popup": "popup.html"        // optional
    },
	"permissions": ["storage", "activeTab", "tabs", "nativeMessaging", "bookmarks", "windows"]
}
